{
    "name": "aliabbasbeing/php-ecommerce-platform",
    "description": "Complete PHP E-commerce Platform with modern features",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "firebase/php-jwt": "^6.0",
        "phpmailer/phpmailer": "^6.8",
        "stripe/stripe-php": "^10.0",
        "twilio/sdk": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}